<UserControl x:Class="TimeAttendanceWpf.UI.Views.ManagerDashboardView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Grid Margin="24">
        <StackPanel>
            <TextBlock Text="{Binding Title}" FontSize="22" Margin="0,0,0,12"/>
            <TextBlock Text="Manager dashboard coming next." Margin="0,0,0,12"/>
            <Button Content="Back" Command="{Binding BackCommand}" Width="120" Height="34"/>
        </StackPanel>
    </Grid>
</UserControl>
